{
  "name": "virt-ui",
  "version": "0.0.1",
  "description": "OpenShift VM Migration UI",
  "main": "server.js",
  "repository": "https://github.com/konveyor/virt-ui.git",
  "license": "Apache",
  "private": true,
  "scripts": {
    "prebuild": "yarn clean",
    "dr:surge": "node dr-surge.js",
    "build": "NODE_ENV=production webpack --config webpack.prod.js && yarn dr:surge",
    "build:remote": "DATA_SOURCE=remote yarn build",
    "build:mock": "DATA_SOURCE=mock yarn build",
    "start": "node server.js",
    "start:dev": "NODE_ENV=development webpack-dev-server --hot --color --progress --info=true --config webpack.dev.js",
    "start:dev:remote": "DATA_SOURCE=remote yarn start:dev",
    "start:dev:mock": "DATA_SOURCE=mock yarn start:dev",
    "test": "jest",
    "eslint": "eslint --ext .tsx,.js ./src/",
    "lint": "yarn eslint",
    "format": "prettier --check --write ./src/**/*.{tsx,ts}",
    "type-check": "yarn tsc --noEmit",
    "ci": "yarn type-check && yarn lint && yarn test",
    "build:bundle-profile": "webpack --config webpack.prod.js --profile --json > stats.json",
    "bundle-profile:analyze": "yarn build:bundle-profile && webpack-bundle-analyzer ./stats.json",
    "clean": "rimraf dist"
  },
  "devDependencies": {
    "@types/dotenv-webpack": "^3.0.0",
    "@types/enzyme-adapter-react-16": "^1.0.6",
    "@types/jest": "^25.2.3",
    "@types/mini-css-extract-plugin": "^0.9.1",
    "@types/optimize-css-assets-webpack-plugin": "^5.0.1",
    "@types/react-dom": "^16.9.8",
    "@types/react-router-dom": "^5.1.5",
    "@types/terser-webpack-plugin": "^4.2.0",
    "@types/victory": "^33.1.4",
    "@types/webpack-merge": "^4.1.5",
    "@types/yup": "^0.29.7",
    "@typescript-eslint/eslint-plugin": "^3.1.0",
    "@typescript-eslint/parser": "^3.1.0",
    "css-loader": "^3.5.3",
    "dotenv-webpack": "^3.0.0",
    "eslint": "^7.1.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.20.0",
    "eslint-plugin-react-hooks": "^4.0.4",
    "file-loader": "^6.0.0",
    "html-webpack-plugin": "^4.5.0",
    "imagemin": "^7.0.0",
    "jest": "^26.0.1",
    "mini-css-extract-plugin": "^0.9.0",
    "optimize-css-assets-webpack-plugin": "^5.0.4",
    "prettier": "^2.0.5",
    "prop-types": "^15.6.1",
    "raw-loader": "^4.0.1",
    "react-axe": "^3.4.1",
    "react-docgen-typescript-loader": "^3.7.2",
    "react-query-devtools": "^2.5.0",
    "react-router-dom": "^5.2.0",
    "regenerator-runtime": "^0.13.5",
    "rimraf": "^3.0.2",
    "style-loader": "^1.2.1",
    "svg-url-loader": "^6.0.0",
    "terser-webpack-plugin": "^4.2.2",
    "ts-jest": "^26.1.0",
    "ts-loader": "^7.0.5",
    "tsconfig-paths-webpack-plugin": "^3.3.0",
    "tslib": "^2.0.0",
    "typescript": "^3.9.3",
    "url-loader": "^4.1.0",
    "webpack": "^4.44.2",
    "webpack-bundle-analyzer": "^3.9.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0",
    "webpack-merge": "^5.1.4"
  },
  "dependencies": {
    "@konveyor/lib-ui": "^1.5.0",
    "@patternfly/react-core": "^4.47.0",
    "@patternfly/react-icons": "^4.7.4",
    "@patternfly/react-styles": "^4.7.3",
    "@patternfly/react-table": "^4.16.7",
    "@patternfly/react-tokens": "^4.9.6",
    "express": "^4.17.1",
    "fast-deep-equal": "^3.1.3",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-query": "^2.23.0",
    "react-router-last-location": "^2.0.1",
    "yup": "^0.29.3"
  }
}
